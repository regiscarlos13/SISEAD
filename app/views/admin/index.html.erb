<div class="container mt-3">
	<div class="card ">
		<h5 class="card-header text-white bg-primary ">Administrador</h5>
		<div class="card-body">


      <!-- Nav tabs -->
      <ul class="nav nav-tabs" id="myTab" role="tablist">
        <li class="nav-item" role="presentation">
          <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true">Provas</a>
        </li>
        <li class="nav-item" role="presentation">
          <a class="nav-link" id="profile-tab" data-toggle="tab" href="#profile" role="tab" aria-controls="profile" aria-selected="false">Calend√°rio</a>
        </li>
        
      </ul>

      <!-- Tab panes -->
      <div class="tab-content mt-4">
        <div class="tab-pane active" id="home" role="tabpanel" aria-labelledby="home-tab">

          <%= render partial: 'provas' %>

        </div>
        <div class="tab-pane" id="profile" role="tabpanel" aria-labelledby="profile-tab">
          <%#= render partial: 'calendario' %>

          <% @series.each do |f| %>

          <%= link_to   f ==  'Infantil' ? 'Infantil' : "#{f} - Ano"  , calendario_path(f), class: 'btn btn-outline-success' %>

          <% end %>



        </div>
      </div>
    </div>
  </div>

